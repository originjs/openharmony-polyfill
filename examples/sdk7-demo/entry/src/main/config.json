{
  "app": {
    "bundleName": "org.originjs.openharmony.polyfilldemo",
    "vendor": "originjs",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "org.originjs.openharmony.polyfilldemo",
    "name": ".MyApplication",
    "mainAbility": "org.originjs.openharmony.polyfilldemo.MainAbility",
    "deviceType": [
      "phone",
      "tablet",
      "car"
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "http",
        "usedScene": {
          "ability": [
            "org.originjs.openharmony.polyfilldemo.MainAbility"
          ],
          "when": "always"
        }
      }
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "visible": true,
        "name": "org.originjs.openharmony.polyfilldemo.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "mode": {
          "syntax": "ets",
          "type": "pageAbility"
        },
        "pages": [
          "pages/index",
          "pages/network",
          "pages/textutil",
          "pages/protobuf",
          "pages/fs",
          "pages/autobahn"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}